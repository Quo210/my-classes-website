<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Classes</title>
    <%- include('partials/bootstrap.ejs') %>
    <link rel="stylesheet" href="style.css">
</head>
<body class="bg-dark">
    <!--DO NOT MODIFY ORIGINAL FILE-->
        <%- include('partials/navbar.ejs') %>
    <main class="container-lg p-1 my-1 bg-dark">
        <h1 class="text-center">Mis Clases</h1>
        <div class="row themes row-gap-3 justify-content-center justify-content-lg-between">
            <% console.log(data)      %>
            <% data.forEach(videoList => {     %>
                <a class="border border-5 border-light col-8 col-md-4 col-lg-3 card container-sm m-md-3 m-md-1 p-1 p-md-2 p-lg-3 bg-dark link-light link-opacity-75 link-opacity-100-hover link-underline-opacity-0 link-underline-opacity-50-hover" href="<%= videoList.dbId %>">
                    <h3 class="text-center">
                        <%=  videoList["name"] %>
                    </h3>
                    <img class="border border-3 border-light" src="<%=  videoList['thumbnail']    %>" alt="">
                </a>
            <% })  %>
        </div>
        
       
    </main>
    <%-  include("partials/footer.ejs")   %>
</body>
</html>